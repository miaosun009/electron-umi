{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": "./",
    "strict": true,
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["src/render/.umi/*"],
      "@components/*": ["src/render/components/*"],
      "@pages/*": ["src/render/pages/*"],
      "@store/*": ["src/render/store/*"]
    },
    "allowSyntheticDefaultImports": true
  }
}
